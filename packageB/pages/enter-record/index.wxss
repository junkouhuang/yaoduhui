.empty-data {
  /* margin-top:200rpx; */
  display: flex;
  flex-direction: column;
  align-items: center;
  height: 100%;
  justify-content: center;
  margin-top: -100rpx;
}

.empty-data image {
  width: 250rpx;
  height: 250rpx;
}

.empty-data text {
  color: #615F60;
  font-size: 28rpx;
  margin-top: 55rpx;
}

.empty-data navigator {
  background: linear-gradient(90deg, rgba(4, 190, 254, 1), rgba(68, 129, 235, 1));
  width: 217rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  border-radius: 30rpx;
  color: #fff;
  font-size: 28rpx;
  margin-top: 70rpx;
}