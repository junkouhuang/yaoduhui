
.customer-page {
  display: flex;
  flex-direction: column;
  height: 100%;
}

.search-enterprise {
  background-color: #fff;
  box-sizing: border-box;
  padding: 17rpx 36rpx 0;
  flex-basis: 103rpx;
  margin-bottom: 20rpx;
  z-index: 999;
}

.search-enterprise .search-shell {
  width: 678rpx;
  height: 72rpx;
  border-radius: 36rpx;
  background: #F3F4F5;
  box-sizing: border-box;
  padding-left: 20rpx;
  display: flex;
  align-items: center;
}

.search-shell image {
  width: 60rpx;
  height: 60rpx;
}

.search-shell input {
  font-size: 28rpx;
  margin-left: 25rpx;
  width: 570rpx;
}

.search-shell .close-shell {
  width: 34rpx;
  height: 34rpx;
  border-radius: 50%;
  background-color: #b2b2b2;
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  right: 65rpx;
  z-index: 10;
}

.search-shell .close {
  width: 15rpx;
  height: 15rpx;
}

.search-enterprise .tabble-toggle {
  display: flex;
  justify-content: space-between;
  box-sizing: border-box;
  padding: 0 140rpx;
  height: 84rpx;
  position: relative;
}

.tabble-toggle .toggle-item {
  font-size: 30rpx;
  color: #333;
  line-height: 84rpx;
  height: 100%;
}

.tabble-toggle .toggle-item.active {
  font-weight: bold;
}

.tabble-toggle .line {
  width: 90rpx;
  height: 5rpx;
  background: linear-gradient(90deg, rgba(4, 190, 254, 1), rgba(68, 129, 235, 1));
  position: absolute;
  /* left:140rpx; */
  bottom: 0;
  transition: 0.3s all;
}

.medical-care {
  display: flex;
  margin-top: 20rpx;
  flex-direction: column;
  flex: 1
}

.medical-item {
  box-sizing: border-box;
  padding: 22rpx 0;
  background-color: #fff;
  justify-content: space-between;
  margin: 0 30rpx 30rpx;
  align-items: center;
  border-radius: 26rpx;
}

.medical-item image {
  width: 90rpx;
  height: 90rpx;
}

.medical-item .medical-left {
  display: flex;
  flex-direction: column;
  flex: 1;
}

.medical-item .medical-left .definition-mode {
  font-size: 28rpx;
  color: #333333;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 22rpx 26rpx;
  position: relative;
}

.medical-item .medical-left .definition-mode::after {
  position: absolute;
  box-sizing: border-box;
  content: ' ';
  pointer-events: none;
  right: 0;
  bottom: 0;
  left: 0;
  border-bottom: 1px solid #ebedf0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}

.medical-item .medical-left .definition-mode .left {
  width: 500rpx;
}

.medical-item .medical-left .definition-mode  .status {
  color: #3996E1;
  font-size: 28rpx;
}

.medical-item .medical-left .definition-mode  .status2 {
  color: #F13C3C;
  font-size: 28rpx;
}

.medical-item .medical-left .definition-mode .definition {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  width: 500rpx;
  font-weight: bold;
}

.medical-item .medical-left .definition-mode .modeName {
  font-size: 26rpx;
  margin-top: 10rpx;
  color: #808080;
}

.medical-left .definition-mode .mode {
  margin-left: 20rpx;
  color: #868686;
  font-size: 22rpx;
  line-height: 16rpx;
}

.medical-left .name-phone {
  margin-top: 25rpx;
  display: flex;
  align-items: center;
  font-size: 30rpx;
  color: #333;
  padding: 0 22rpx 26rpx;
}

.medical-left .name-phone .phone {
  margin-left: 5rpx;
  font-size: 30rpx;
}

.medical-left .name-phone .name {
  width: 200rpx;
}

.medical-left .reason view {
  color: #EA0505;
  font-size: 26rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
  margin: 0 22rpx 18rpx;
  background: #F3F4F5;
  padding:8rpx 16rpx;
  border-radius: 16rpx;
  float: left;
}

.medical-left .reason .phone {
  /* margin-left:10rpx; */
  font-size: 30rpx;
  color: #EA0505
}


.medical-left .name-item {
  margin-top: 25rpx;
  display: flex;
  align-items: center;
  font-size: 26rpx;
  color: #333;
  margin: 0 22rpx 18rpx;
}


.medical-left .name-contacts {
  margin-top: 25rpx;
  display: flex;
  align-items: center;
  font-size: 26rpx;
  color: #333;
  margin: 16rpx 22rpx 18rpx 22rpx;
}

.medical-left .name-item .phone {
  margin-left: 5rpx;
  font-size: 30rpx;
}

.medical-left .address {
  margin-top: 25rpx;
  font-size: 26rpx;
  color: #333;
  max-width: 250px
}

.medical-left .shipping-companies {
  font-size: 24rpx;
  line-height: 24rpx;
  color: #868686;
  margin-top: 25rpx;
}

