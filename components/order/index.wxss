swiper {
  margin-top: 212rpx;
  width: 100%;
  height: calc(100vh - 214rpx);
}

swiper-item {
  height: calc(100vh - 116rpx);
}

.scoll-h {
  height: 100%;
  overflow-y: auto;
  -webkit-overflow-scrolling: touch;
}

.what {
  color: #10B3FB;
  font-size: 30rpx;
  padding: 20rpx;
}

.tab-box .item {
  border-top: 18rpx solid #f5f5f5;
  overflow: hidden;
  margin: 0 20rpx;
  border-radius: 26rpx;
}

.tab-box .item .contian {
  background-color: #fff;
  border-radius: 26rpx 26rpx 0 0;
  border-bottom: 2rpx solid rgba(244, 244, 244, 1);
}

.tab-box .psf {
  color: #333;
  font-size: 30rpx;
  display: flex;
  justify-content: space-between;
  padding: 36rpx 28rpx;
  border-bottom: 2rpx solid rgba(244, 244, 244, 1);
}

.tab-box .psf .supplierName {
  display: flex;
  align-items: center;
  flex: 1;
}

.tab-box .psf .supplierName view {
  max-width: 500rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  font-weight: bold;
}

.tab-box .psf .supplierName image {
  width: 12rpx;
  height: 21rpx;
  margin-left: 16rpx;
}

.goods {
  padding: 34rpx 20rpx 0 20rpx;
  background-color: #fff;
}

.good_item {
  display: flex;
}

.goods .picture {
  width: 180rpx;
  height: 180rpx;
  border-radius: 8rpx;
}

.goods .picture .imgUrl {
  width: 180rpx;
  height: 180rpx;
  border: 2rpx solid #f5f5f5;
  border-radius: 16rpx;
  background-size: cover;
  background-position: center;
  position: relative;
}

.goods .picture .imgUrl image {
  width: 56rpx;
  height: 26rpx;
  position: absolute;
  top: 0;
  right: 0;
}

.goods .xq {
  display: flex;
  flex-direction: column;
  margin-left: 34rpx;
}

.goods .xq .mc {
  color: #333;
  font-size: 30rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
  font-weight: bold;
}

.goods .xq .gs {
  color: #858585;
  font-size: 26rpx;
  margin-top: 18rpx;
  display: flex;
}
.goods .xq .spec {
  color: #858585;
  font-size: 26rpx;
  margin-top: 14rpx;
  display: flex;
}

.goods .xq .gs view{
  background-color: #f2f2f2;
  font-size: 18rpx;
  padding: 2rpx 8rpx;
  margin-right: 12rpx;
  color: #868686;
  border-radius: 6rpx;
}

.goods .xq .gs .manufacturer {
  width: 324rpx;
  font-size: 24rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.goods .xq .jg {
  color: #858585;
  font-size: 26rpx;
  margin-top: 14rpx;
}

.goods .xq .jg text {
  font-size: 26rpx;
}

.goods .xq .gg {
  margin-top: 16rpx;
  display: none;
}

.goods .xq .gg .spec {
  color: #868686;
  border-radius: 20rpx;
  background-color: #f2f2f2;
  padding: 0 16rpx;
  font-size: 20rpx;
  margin-top: 20rpx;
  margin-right: 14rpx;
  float: left;
  height: 40rpx;
  line-height: 40rpx;
}

.goods .xq .gg text {
  color: #868686;
  border-radius: 60rpx;
  background-color: #f2f2f2;
  padding: 12rpx 16rpx;
  font-size: 22rpx;
  margin-top: 20rpx;
  margin-right: 14rpx;
}

