/**
  *列表加载更多样式
  */
.loadding {
  padding: 28rpx;
  text-align: center;
  font-size: 28rpx;
  color: #333333;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #f5f5f5;
}

.loadding image {
  width: 36rpx;
  height: 36rpx;
}

.loadding text {
  margin-left: 10rpx;
  color: #969799;
}

.maskLayer {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.5);
  z-index: 3;
}

.choose {
  width: 96%;
  margin-left: 2%;
  position: fixed;
  bottom: 0;
  z-index: 4;
  border-radius: 30rpx;
  margin-bottom: 16rpx;
  background-color: #fff;
  overflow: hidden;
}

.choose .box_img {
  width: 66%;
  height: 340rpx;
  margin: 20rpx 17%;
  background-size: contain;
  background-repeat: no-repeat;
  background-position: center;
}

.choose .btn {
  display: flex;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  z-index: 5;
}

.choose .btn .cancel {
  flex: 1;
}

.choose .btn .ok {
  flex: 1;
  font-size: 34rpx;
  color: #fff;
  background-image: linear-gradient(to right, #04befe, #4481eb);
  box-shadow: 0 0.206667rem 0.26rem 0 rgba(67, 115, 234, 0.3);
  opacity: 0.54;
}

.choose .btn  .btnActive {
  opacity: 1;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

