  .default image {
    width: 182rpx;
    height: 210rpx;
  }
  
  .default text {
    margin-top: 60rpx;
    color: #615f60;
    font-size: 28rpx;
  }
  
  .default .go {
    width: 216rpx;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 28rpx;
    color: #fff;
    margin-top: 74rpx;
    border-radius: 60rpx;
    background-image: linear-gradient(to right, #04befe, #4481eb);
    box-shadow: 0 0.206667rem 0.26rem 0 rgba(67, 115, 234, 0.3);
  }

