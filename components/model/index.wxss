.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.3);
  z-index: 12;
}

.model {
  width: 630rpx;
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  background: rgba(255, 255, 255, 1);
  border-radius: 26rpx;
  overflow: hidden;
  z-index: 12;
  display: flex;
  flex-direction: column;
}

.model .title {
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
}

.model .imge {
  width: 480rpx;
  height: 640rpx;
  margin: auto;
}

.model .iSee {
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
}