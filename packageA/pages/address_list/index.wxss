
.address_list {
  padding-bottom: 20rpx;
}

.address_list .addressList {
  position: relative;
  padding: 30rpx 0;
  margin: 20rpx 20rpx 0;
  background-color: #fff;
  border-radius: 26rpx;
  box-shadow: 0px 2px 8px rgba(103, 102, 102, 0.1);
}

.addressList .addressTop,
.addressList .addressBottom{
  padding: 0 20rpx;
}

.addressList .addressTop{
  /**border-bottom: 1rpx solid #ECECEC;*/
  /* margin-bottom: 25rpx; */
  padding-bottom: 25rpx;
  position: relative;
}

.addressList .addressTop::after {
  position: absolute;
  box-sizing: border-box;
  content: ' ';
  pointer-events: none;
  right: 0;
  bottom: 0;
  left: 0;
  border-bottom: 1px solid #ebedf0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}

