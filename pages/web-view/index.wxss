.bohai_hint {
  background: url('https://edm.xinquanjk.com/upload/iconfont/202003/20/bohai_notice1.png') no-repeat;
  width: 100%;
  height: 450rpx;
  background-size: contain;
}

.conten2t {
  padding: 20rpx;
  font-size: 28rpx;
}

.hinte {
  font-size: 28rpx;
}

.hinte text {
  color: red;
}

.conten2t image {
  width: 20rpx;
  height: 20rpx;
}

.maskLayer {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.6);
  z-index: 5;
}

.buildSuccess {
  transform: translateX(-50%) translateY(-50%);
  width: 640rpx;
  border-radius: 24rpx;
  background-color: #fff;
  z-index: 7;
  color: #333;
  position: fixed;
  top: 50%;
  left: 50%;
  height: calc(70vh);
  overflow: hidden;
  display: flex;
}

.buildSuccess .title11 {
  text-align: center;
  font-size: 30rpx;
  font-weight: bold;
  height: 100rpx;
  line-height: 100rpx;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  border-bottom: 1rpx solid #EFEFEF;
  background: #fff;
}

.buildSuccess .richText {
  padding: 10rpx 40rpx;
  overflow-y: auto;
  -webkit-overflow-scrolling: touch;
  margin: 100rpx 0 100rpx;
}

.buildSuccess .content11 {
  margin: 34rpx 34rpx 0 34rpx;
  font-size: 28rpx;
}

.buildSuccess .title22 {
  font-size: 28rpx;
  margin: 24rpx 50rpx 0 50rpx;
  position: relative;
  font-weight: bold;
}

.buildSuccess .content22 {
  margin: 24rpx 34rpx 0 34rpx;
  font-size: 28rpx;
}

.buildSuccess .content22 text {
  color: #f00;
}

.buildSuccess .title33 {
  font-size: 28rpx;
  margin: 24rpx 50rpx 0 50rpx;
  position: relative;
  font-weight: bold;
}

.buildSuccess .content33 {
  margin: 24rpx 34rpx 0 34rpx;
  font-size: 28rpx;
}

.buildSuccess .content44 {
  margin: 24rpx 34rpx 0 34rpx;
  font-size: 28rpx;
}

.buildSuccess .content55 {
  padding: 0 17px 22px 17px;
  font-size: 28rpx;
  border-bottom: 2rpx solid #ececec;
}

.buildSuccess .title22::after {
  content: "";
  width: 4rpx;
  height: 26rpx;
  background-color: #3996e1;
  position: absolute;
  top: 50%;
  margin-top: -13rpx;
  left: 0;
  margin-left: -14rpx;
}

.buildSuccess .title33::after {
  content: "";
  width: 4rpx;
  height: 26rpx;
  background-color: #3996e1;
  position: absolute;
  top: 50%;
  margin-top: -13rpx;
  left: 0;
  margin-left: -14rpx;
}

.buildSuccess .btn {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  line-height: 56px;
  min-height: 56px;
  color: #576b95;
  display: flex;
  font-size: 30rpx;
  justify-content: center;
  font-weight: 700;
  background: #fff;
}

.btn::after {
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 1px;
  border-top: 1px solid rgba(0, 0, 0, 0.1);
  color: rgba(0, 0, 0, 0.1);
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}