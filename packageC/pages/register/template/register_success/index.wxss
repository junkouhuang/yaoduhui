.register_success {
  background-color: #ffffff;
  height: calc(100vh)
}
.flow3 {
  padding: 110rpx 0;
  text-align: center;
}
.flow3 image {
  width: 566rpx;
  height: 44rpx;
}
.flow3 .title {
  text-align: center;
  font-size: 22rpx;
  color: #999999;
}
.flow3 .title {
  width: 566rpx;
  position: relative;
  margin: 0 auto;
}
.flow3 .active {
  color: #4481EB;
  left: -47rpx;
  position: absolute;
}
.flow3 .set {
   color: #4481EB;
}
.flow3 .last {
  position: absolute;
  right: -20rpx;
  color: #4481EB;
}
.flow3 .suceess {
  margin-top: 88rpx;
  width: 160rpx;
  height: 160rpx;
}
.success {
  width: 100%;
  text-align: center;
